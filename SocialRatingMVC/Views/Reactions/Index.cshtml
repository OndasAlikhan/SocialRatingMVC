@{
    var reactions = ViewData["reactions"] as IList<Reaction>;
}
<div class="reactions">
    <div class="reaction-list">
        @foreach (var reaction in reactions)
        {
            <div class="reaction-item card">
                <div class="card-body">@reaction.ReactionText</div>
            </div>
        }
    </div>
</div>